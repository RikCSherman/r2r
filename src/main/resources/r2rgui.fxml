<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="275.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="r2r.gui.RtoRController">
   <children>
      <Button layoutX="171.0" layoutY="52.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="27.0" prefWidth="100.0" text="Next System" />
      <TextField fx:id="startSystem" layoutX="10.0" layoutY="14.0" prefHeight="27.0" prefWidth="280.0" promptText="Start System" />
      <TextArea fx:id="planets" editable="false" layoutX="10.0" layoutY="127.0" prefHeight="134.0" prefWidth="280.0" promptText="Planets" />
      <TextField fx:id="next" editable="false" layoutX="10.0" layoutY="89.0" prefHeight="27.0" prefWidth="280.0" promptText="Next System" />
      <Spinner fx:id="distance" amountToStepBy="50" initialValue="200" layoutX="10.0" layoutY="52.0" max="1000" min="100" prefWidth="100.0" />
   </children>
</AnchorPane>
